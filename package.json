{
  "name": "generate-json-patch",
  "version": "0.0.1",
  "author": "Marco Link",
  "private": false,
  "repository": "marcolink/contentful-collection",
  "homepage": "https://github.com/marcolink/generate-json-patch",
  "license": "ISC",
  "description": "A simple function to diff any object and generate a JSON Patch",
  "main": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "default": "./dist/index.js",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "node": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "test": "ts-mocha -p tsconfig.json src/index.spec.ts",
    "test-coverage": "nyc ts-mocha -p tsconfig.json src/index.spec.ts",
    "test-watch": "npm run test -- -w --watch-files '**/*.ts'"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "fast-json-patch": "^3.1.1",
    "mocha-ts": "^0.0.0",
    "nyc": "^15.1.0",
    "ts-mocha": "^10.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
